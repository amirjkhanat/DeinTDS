{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport BaseLayout from \"@/components/BaseLayout.vue\";\nexport default {\n  name: 'OffersComponent',\n  components: {\n    BaseLayout\n  },\n  data() {\n    return {\n      offers: [],\n      partnerPrograms: [],\n      newOffer: {\n        name: '',\n        offerType: 'html_file',\n        partnerProgram: '',\n        htmlFile: null,\n        redirectUrl: '',\n        preloadUrl: '',\n        actionType: ''\n      },\n      sortKey: '',\n      sortAsc: true\n    };\n  },\n  computed: {\n    sortedOffers() {\n      let arrayCopy = [...this.offers];\n      const key = this.sortKey;\n      const asc = this.sortAsc;\n      arrayCopy.sort(function (a, b) {\n        if (a[key] < b[key]) return asc ? -1 : 1;\n        if (a[key] > b[key]) return asc ? 1 : -1;\n        return 0;\n      });\n      return arrayCopy;\n    }\n  },\n  methods: {\n    fetchOffers() {\n      fetch('http://127.0.0.1/admin/api/offers/', {\n        headers: {\n          'X-Requested-With': 'XMLHttpRequest'\n        }\n      }).then(response => response.json()).then(data => {\n        this.offers = data;\n      });\n    },\n    fetchPartnerPrograms() {\n      fetch('http://127.0.0.1/admin/api/partner-programs/', {\n        headers: {\n          'X-Requested-With': 'XMLHttpRequest'\n        }\n      }).then(response => response.json()).then(data => {\n        this.partnerPrograms = data;\n      });\n    },\n    submitForm() {\n      this.offers.push(this.newOffer);\n      this.newOffer = {\n        name: '',\n        offerType: 'html_file',\n        partnerProgram: '',\n        htmlFile: null,\n        redirectUrl: '',\n        preloadUrl: '',\n        actionType: ''\n      };\n    },\n    handleOfferTypeChange() {\n      this.newOffer.htmlFile = this.newOffer.offerType === 'html_file' ? '' : null;\n      this.newOffer.redirectUrl = this.newOffer.offerType === 'redirect' ? '' : null;\n      this.newOffer.preloadUrl = this.newOffer.offerType === 'preload' ? '' : null;\n      this.newOffer.actionType = this.newOffer.offerType === 'action' ? '' : null;\n    },\n    handleFileUpload(event) {\n      this.newOffer.htmlFile = event.target.files[0];\n    },\n    sortById() {\n      this.sortKey = 'id';\n      this.sortAsc = !this.sortAsc;\n    },\n    sortByName() {\n      this.sortKey = 'name';\n      this.sortAsc = !this.sortAsc;\n    },\n    sortByOfferType() {\n      this.sortKey = 'offer_type';\n      this.sortAsc = !this.sortAsc;\n    }\n  },\n  created() {\n    this.fetchOffers();\n    this.fetchPartnerPrograms();\n  }\n};","map":{"version":3,"names":["BaseLayout","name","components","data","offers","partnerPrograms","newOffer","offerType","partnerProgram","htmlFile","redirectUrl","preloadUrl","actionType","sortKey","sortAsc","computed","sortedOffers","arrayCopy","key","asc","sort","a","b","methods","fetchOffers","fetch","headers","then","response","json","fetchPartnerPrograms","submitForm","push","handleOfferTypeChange","handleFileUpload","event","target","files","sortById","sortByName","sortByOfferType","created"],"sources":["C:\\Users\\Амир\\PycharmProjects\\core\\static\\frontend\\src\\components\\OffersComponent.vue"],"sourcesContent":["<template>\r\n  <base-layout>\r\n    <div class=\"d-flex justify-content-between align-items-center\">\r\n      <h1>Offers</h1>\r\n      <button type=\"button\" class=\"btn btn-primary\" data-bs-toggle=\"modal\" data-bs-target=\"#offerModal\">\r\n        Add Offer\r\n      </button>\r\n    </div>\r\n    <table class=\"table\">\r\n      <thead>\r\n        <tr>\r\n          <th class=\"col-id\" @click=\"sortById\">ID</th>\r\n          <th class=\"col-name\" @click=\"sortByName\">Name</th>\r\n          <th class=\"col-offer-type\" @click=\"sortByOfferType\">Offer Type</th>\r\n          <th>Actions</th>\r\n        </tr>\r\n      </thead>\r\n      <tbody>\r\n        <tr v-for=\"offer in sortedOffers\" :key=\"offer.id\">\r\n          <td>{{ offer.id }}</td>\r\n          <td>{{ offer.name }}</td>\r\n          <td>{{ offer.offer_type }}</td>\r\n          <td>\r\n            <button class=\"btn btn-info\">Show</button>\r\n            <button class=\"btn btn-warning\">Edit</button>\r\n            <button class=\"btn btn-danger\">Delete</button>\r\n          </td>\r\n        </tr>\r\n      </tbody>\r\n    </table>\r\n\r\n    <div class=\"modal fade\" id=\"offerModal\" tabindex=\"-1\" aria-labelledby=\"offerModalLabel\" aria-hidden=\"true\">\r\n      <div class=\"modal-dialog\">\r\n        <div class=\"modal-content\">\r\n          <div class=\"modal-header\">\r\n            <h5 class=\"modal-title\" id=\"offerModalLabel\">Add Offer</h5>\r\n            <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\r\n          </div>\r\n          <div class=\"modal-body\">\r\n            <form @submit.prevent=\"submitForm\" class=\"form-group\">\r\n              <label class=\"form-label\">\r\n                Name:\r\n                <input v-model=\"newOffer.name\" type=\"text\" class=\"form-control\">\r\n              </label>\r\n              <label class=\"form-label\">\r\n                Offer Type:\r\n                <select v-model=\"newOffer.offerType\" @change=\"handleOfferTypeChange\" class=\"form-select\">\r\n                  <option value=\"html_file\">HTML File</option>\r\n                  <option value=\"redirect\">Redirect</option>\r\n                  <option value=\"preload\">Preload</option>\r\n                  <option value=\"action\">Action</option>\r\n                </select>\r\n              </label>\r\n              <label class=\"form-label\">\r\n                Partner Program:\r\n                <select v-model=\"newOffer.partnerProgram\" class=\"form-select\">\r\n                  <option v-for=\"program in partnerPrograms\" :value=\"program.id\" :key=\"program.id\">\r\n                    {{ program.name }}\r\n                  </option>\r\n                </select>\r\n              </label>\r\n                    <select v-if=\"newOffer.offerType === 'action'\" v-model=\"newOffer.actionType\" class=\"form-select\">\r\n        <option value=\"404\">404 Not Found</option>\r\n        <option value=\"redirect\">Redirect to another campaign</option>\r\n        <option value=\"html\">Show as HTML</option>\r\n      </select>\r\n              <input v-if=\"newOffer.offerType === 'html_file'\" type=\"file\" @change=\"handleFileUpload\" class=\"form-control\">\r\n              <input v-if=\"newOffer.offerType === 'redirect'\" type=\"text\" v-model=\"newOffer.redirectUrl\" class=\"form-control\">\r\n              <input v-if=\"newOffer.offerType === 'preload'\" type=\"text\" v-model=\"newOffer.preloadUrl\" class=\"form-control\">\r\n              <input v-if=\"newOffer.offerType === 'action'\" type=\"text\" v-model=\"newOffer.actionType\" class=\"form-control\">\r\n            </form>\r\n          </div>\r\n          <div class=\"modal-footer\">\r\n            <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">Close</button>\r\n            <button type=\"submit\" class=\"btn btn-primary\" form=\"offerForm\">Save changes</button>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </base-layout>\r\n</template>\r\n\r\n<script>\r\nimport BaseLayout from \"@/components/BaseLayout.vue\";\r\n\r\nexport default {\r\n  name: 'OffersComponent',\r\n  components: {BaseLayout},\r\n  data() {\r\n    return {\r\n      offers: [],\r\n      partnerPrograms: [],\r\n      newOffer: {\r\n        name: '',\r\n        offerType: 'html_file',\r\n        partnerProgram: '',\r\n        htmlFile: null,\r\n        redirectUrl: '',\r\n        preloadUrl: '',\r\n        actionType: ''\r\n      },\r\n      sortKey: '',\r\n      sortAsc: true\r\n    };\r\n  },\r\n  computed: {\r\n    sortedOffers() {\r\n      let arrayCopy = [...this.offers];\r\n      const key = this.sortKey;\r\n      const asc = this.sortAsc;\r\n      arrayCopy.sort(function(a, b) {\r\n        if (a[key] < b[key]) return asc ? -1 : 1;\r\n        if (a[key] > b[key]) return asc ? 1 : -1;\r\n        return 0;\r\n      });\r\n      return arrayCopy;\r\n    }\r\n  },\r\n  methods: {\r\n    fetchOffers() {\r\n      fetch('http://127.0.0.1/admin/api/offers/', {\r\n        headers: {\r\n          'X-Requested-With': 'XMLHttpRequest'\r\n        }\r\n      })\r\n      .then(response => response.json())\r\n      .then(data => {\r\n        this.offers = data;\r\n      });\r\n    },\r\n    fetchPartnerPrograms() {\r\n      fetch('http://127.0.0.1/admin/api/partner-programs/', {\r\n        headers: {\r\n          'X-Requested-With': 'XMLHttpRequest'\r\n        }\r\n      })\r\n      .then(response => response.json())\r\n      .then(data => {\r\n        this.partnerPrograms = data;\r\n      });\r\n    },\r\n    submitForm() {\r\n      this.offers.push(this.newOffer);\r\n      this.newOffer = {\r\n        name: '',\r\n        offerType: 'html_file',\r\n        partnerProgram: '',\r\n        htmlFile: null,\r\n        redirectUrl: '',\r\n        preloadUrl: '',\r\n        actionType: ''\r\n      };\r\n    },\r\n    handleOfferTypeChange() {\r\n      this.newOffer.htmlFile = this.newOffer.offerType === 'html_file' ? '' : null;\r\n      this.newOffer.redirectUrl = this.newOffer.offerType === 'redirect' ? '' : null;\r\n      this.newOffer.preloadUrl = this.newOffer.offerType === 'preload' ? '' : null;\r\n      this.newOffer.actionType = this.newOffer.offerType === 'action' ? '' : null;\r\n    },\r\n    handleFileUpload(event) {\r\n      this.newOffer.htmlFile = event.target.files[0];\r\n    },\r\n    sortById() {\r\n      this.sortKey = 'id';\r\n      this.sortAsc = !this.sortAsc;\r\n    },\r\n    sortByName() {\r\n      this.sortKey = 'name';\r\n      this.sortAsc = !this.sortAsc;\r\n    },\r\n    sortByOfferType() {\r\n      this.sortKey = 'offer_type';\r\n      this.sortAsc = !this.sortAsc;\r\n    }\r\n  },\r\n  created() {\r\n    this.fetchOffers();\r\n    this.fetchPartnerPrograms();\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.col-id {\r\n  width: 10%;\r\n}\r\n\r\n.col-name {\r\n  width: 40%;\r\n}\r\n\r\n.col-offer-type {\r\n  width: 30%;\r\n}\r\n</style>"],"mappings":";AAmFA,OAAOA,UAAS,MAAO,6BAA6B;AAEpD,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IAACF;EAAU,CAAC;EACxBG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,EAAE;MACVC,eAAe,EAAE,EAAE;MACnBC,QAAQ,EAAE;QACRL,IAAI,EAAE,EAAE;QACRM,SAAS,EAAE,WAAW;QACtBC,cAAc,EAAE,EAAE;QAClBC,QAAQ,EAAE,IAAI;QACdC,WAAW,EAAE,EAAE;QACfC,UAAU,EAAE,EAAE;QACdC,UAAU,EAAE;MACd,CAAC;MACDC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE;IACX,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,YAAYA,CAAA,EAAG;MACb,IAAIC,SAAQ,GAAI,CAAC,GAAG,IAAI,CAACb,MAAM,CAAC;MAChC,MAAMc,GAAE,GAAI,IAAI,CAACL,OAAO;MACxB,MAAMM,GAAE,GAAI,IAAI,CAACL,OAAO;MACxBG,SAAS,CAACG,IAAI,CAAC,UAASC,CAAC,EAAEC,CAAC,EAAE;QAC5B,IAAID,CAAC,CAACH,GAAG,IAAII,CAAC,CAACJ,GAAG,CAAC,EAAE,OAAOC,GAAE,GAAI,CAAC,IAAI,CAAC;QACxC,IAAIE,CAAC,CAACH,GAAG,IAAII,CAAC,CAACJ,GAAG,CAAC,EAAE,OAAOC,GAAE,GAAI,IAAI,CAAC,CAAC;QACxC,OAAO,CAAC;MACV,CAAC,CAAC;MACF,OAAOF,SAAS;IAClB;EACF,CAAC;EACDM,OAAO,EAAE;IACPC,WAAWA,CAAA,EAAG;MACZC,KAAK,CAAC,oCAAoC,EAAE;QAC1CC,OAAO,EAAE;UACP,kBAAkB,EAAE;QACtB;MACF,CAAC,EACAC,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAChCF,IAAI,CAACxB,IAAG,IAAK;QACZ,IAAI,CAACC,MAAK,GAAID,IAAI;MACpB,CAAC,CAAC;IACJ,CAAC;IACD2B,oBAAoBA,CAAA,EAAG;MACrBL,KAAK,CAAC,8CAA8C,EAAE;QACpDC,OAAO,EAAE;UACP,kBAAkB,EAAE;QACtB;MACF,CAAC,EACAC,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAChCF,IAAI,CAACxB,IAAG,IAAK;QACZ,IAAI,CAACE,eAAc,GAAIF,IAAI;MAC7B,CAAC,CAAC;IACJ,CAAC;IACD4B,UAAUA,CAAA,EAAG;MACX,IAAI,CAAC3B,MAAM,CAAC4B,IAAI,CAAC,IAAI,CAAC1B,QAAQ,CAAC;MAC/B,IAAI,CAACA,QAAO,GAAI;QACdL,IAAI,EAAE,EAAE;QACRM,SAAS,EAAE,WAAW;QACtBC,cAAc,EAAE,EAAE;QAClBC,QAAQ,EAAE,IAAI;QACdC,WAAW,EAAE,EAAE;QACfC,UAAU,EAAE,EAAE;QACdC,UAAU,EAAE;MACd,CAAC;IACH,CAAC;IACDqB,qBAAqBA,CAAA,EAAG;MACtB,IAAI,CAAC3B,QAAQ,CAACG,QAAO,GAAI,IAAI,CAACH,QAAQ,CAACC,SAAQ,KAAM,W